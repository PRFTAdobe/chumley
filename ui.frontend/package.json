{
  "name": "chumley-frontend",
  "author": "Shannon Sumner",
  "license": "MIT",
  "version": "1.0.0",
  "description": "Chumley AEM Frontend project with Vite",
  "keywords": [
    "spa",
    "aem",
    "vue",
    "adobe"
  ],
  "type": "module",
  "browserslist": [
    "> 0.25%",
    "not dead"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && npm run lint && vite build && tsx ./clientlib.config.ts && tsx ./aem-sync-push.ts",
    "build:prod": "tsc && npm run lint && vite build && tsx ./clientlib.config.ts",
    "lint": "stylelint src/**/*.css --fix && eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest --passWithNoTests --coverage"
  },
  "dependencies": {
    "@adobe/aem-core-components-react-spa": "1.2.0",
    "@adobe/aem-react-editable-components": "1.1.6",
    "@adobe/aem-spa-component-mapping": "1.1.1",
    "@adobe/aem-spa-page-model-manager": "1.3.11",
    "@hookform/resolvers": "^3.4.0",
    "@tanstack/react-query": "5.40.1",
    "aem-react-core-wcm-components-base": "github:PRFTAdobe/aem-react-core-wcm-components-base",
    "classnames": "2.5.1",
    "colord": "^2.9.3",
    "dompurify": "3.1.3",
    "html-react-parser": "5.1.10",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.51.4",
    "react-router-dom": "6.23.1",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.4.5",
    "@testing-library/react": "15.0.7",
    "@testing-library/user-event": "14.5.2",
    "@types/dompurify": "3.0.5",
    "@types/jest": "29.5.12",
    "@types/node": "20.12.11",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "@vitejs/plugin-react-swc": "3.6.0",
    "aem-clientlib-generator": "1.8.0",
    "aemsync": "^5.0.5",
    "autoprefixer": "10.4.19",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-react-refresh": "0.4.7",
    "eslint-plugin-sort-keys": "2.3.5",
    "eslint-plugin-typescript-sort-keys": "3.2.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-fetch-mock": "3.0.3",
    "jest-transform-stub": "2.0.0",
    "postcss": "8.4.38",
    "postcss-dir-pseudo-class": "8.0.1",
    "postcss-html": "1.7.0",
    "postcss-logical": "7.0.1",
    "prettier": "3.2.5",
    "rollup-plugin-copy": "^3.5.0",
    "stylelint": "16.5.0",
